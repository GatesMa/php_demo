{
  "name": "spaphp/spaphp",
  "type": "application",
  "require": {
    "php": ">=7.1.3",
    "spaphp/framework": "dev-v2socket",
    "symfony/finder": "~4.1"
  },
  "require-dev": {
    "mockery/mockery": "~1.2",
    "phpunit/phpunit": ">=5.7",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "app\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "app\\": "test",
      "mock\\": "mock"
    }
  },
  "bin": [
    "spaphp"
  ],
  "repositories": [
    {
      "type": "git",
      "url":"http://git.code.oa.com/spaphp/framework.git"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },

    {
      "type": "composer",
      "url": "http://packagist.tsa.oa.com"
    },
    {
      "packagist": false
    }
  ],
  "config": {
    "secure-http": false
  }
}
